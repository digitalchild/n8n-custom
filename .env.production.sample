# PostgreSQL Configuration
POSTGRES_USER=postgres
POSTGRES_PASSWORD=YourSecurePostgresPassword
POSTGRES_DB=n8n_prod

# Redis Configuration
REDIS_PASSWORD=YourRedisPassword

# n8n Core Configuration
N8N_HOST=0.0.0.0
N8N_PORT=8765
N8N_PROTOCOL=https
N8N_PATH=/
WEBHOOK_URL=https://n8n.yourdomain.com/
N8N_METRICS=false

# Security
N8N_ENCRYPTION_KEY=YourVerySecureEncryptionKey_32Chars
N8N_JWT_SECRET=YourSecureJWTSecret_32Characters
N8N_BASIC_AUTH_ACTIVE=false
N8N_BASIC_AUTH_USER=
N8N_BASIC_AUTH_PASSWORD=

# Email Configuration
N8N_EMAIL_MODE=smtp
N8N_SMTP_HOST=smtp.yourprovider.com
N8N_SMTP_PORT=587
N8N_SMTP_USER=your-smtp-user
N8N_SMTP_PASS=your-smtp-password
N8N_SMTP_SENDER=n8n@yourdomain.com
N8N_SMTP_SSL=false

# Workflow Settings
GENERIC_TIMEZONE=UTC
